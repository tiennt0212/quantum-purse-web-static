{
  "name": "quantum-purse",
  "version": "0.0.1",
  "description": "ckb quantum resistant wallet",
  "private": true,
  "scripts": {
    "start": "npm run build:wasm && webpack serve --open --config ./webpack/webpack.dev.js",
