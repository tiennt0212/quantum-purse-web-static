    "fake-indexeddb": "^6.0.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.3.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^8.0.18",
    "ts-node": "^10.9.2",
    "typescript": "^4.2.3",
    "webpack": "^5.27.1",
    "webpack-cli": "^4.5.0",
